@{
    ViewData["Title"] = "Sessie openen";

}
    <asp:ContentPlaceHolder ID="NavStyle" runat="server">
        <link href="~/css/normalNav.css" rel="stylesheet" />
    </asp:ContentPlaceHolder>
    <div class="container">
        <h4 class="display-4">@ViewData["Title"]</h4>
        <p class="alert"> Ben je zeker dat je de sessie @ViewData["SessionInfo"] wil open zetten? </p>

        <form method="post">
            <button class="btn btn-primary" type="submit">Open</button>
            <a asp-action="Index" class="btn bg-secondary">Cancel</a>
        </form>
    </div>

